{
  "name": "react-component-generator",
  "displayName": "React Component Generator",
  "description": "A simple extension for create a file already prepared to code react",
  "version": "0.0.2",
  "keywords": ["react","snippet","LuHenriSouza"],
  "author": "LuHenriSouza",
  "repository": {
    "type": "git",
    "url": "https://github.com/LuHenriSouza/React-File-Creator-Extension.git"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createReactComponent",
        "title": "Create React Component"
      }
    ],
    "menus": {
      "explorer/context": [
          {
              "command": "extension.createReactComponent",
              "group": "9_create"
          }
      ]
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
